{
    "name": "maryamnbyn/helpers",
    "description": "Vandar Helpers.",
    "keywords": ["laravel", "helpers", "vandar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maryam Nabiyan",
            "email": "maryam.nbyn@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
